trials <- vector("list", length = n_trials)
    
    for (t in seq_len(n_trials)) {
      y_list <- vector("list", length = n_cohorts)
      for (k in seq_len(n_cohorts)) {
        n_k        <- n_subj_vec[k]
        y_list[[k]] <- stats::rnorm(n_k, mean = mu_vec[k], sd = sd_vec[k])
      }
      trials[[t]] <- list(y_list = y_list)
    }

