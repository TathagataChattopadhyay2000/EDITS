breaks <- attr(tu, "bin_breaks")[["y_1"]]
reps   <- attr(tu, "bin_means")[["y_1"]]   # mean-of-samples per bin

y_raw <- scene1$scenario_1$y[, 1]
bin_id <- cut(y_raw, breaks = breaks, include.lowest = TRUE, labels = FALSE)
y_rep_manual <- reps[bin_id]

cbind(y_raw=y_raw, y_rep_manual=y_rep_manual)[1:20, ]
