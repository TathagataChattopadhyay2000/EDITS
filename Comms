  if(is.null(rng_id)){
    for (i in 1:n_chains) {
      inits[[i]]$.RNG.name <- "base::Wichmann-Hill"
      inits[[i]]$.RNG.seed <- stats::runif(1, 0, 2^31)
    }
  } else{
    
    base_seed <- digest::digest2int(as.character(rng_id))
    
    for (i in 1:n_chains) {
      inits[[i]]$.RNG.name <- "base::Wichmann-Hill"
      seed_val <- base_seed + (i - 1L)
      seed_val <- (seed_val %% (2^31 - 1L)) +1L
      inits[[i]]$.RNG.seed <- as.integer(seed_val)
